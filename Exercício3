function separarParesImpares(arr) {
    const resultado = {
        pares: [],
        impares: []
    };
    arr.forEach(num => {
        if (num % 2 === 0) {
            resultado.pares.push(num);
        } else {
            resultado.impares.push(num);
        }
    });
    return resultado;
}

// Exemplo de uso:
console.log(separarParesImpares([1, 2, 3, 4, 5, 6]));
// SaÃ­da: { pares: [2, 4, 6], impares: [1, 3, 5] }
