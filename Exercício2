function ordenarPorIdade(pessoas) {
    return pessoas.sort((a, b) => a.idade - b.idade);
}

// Exemplo de uso:
const pessoas = [
    { nome: 'Ana', idade: 25 },
    { nome: 'Carlos', idade: 18 },
    { nome: 'Maria', idade: 30 }
];
console.log(ordenarPorIdade(pessoas));
// Sa√≠da: [{ nome: 'Carlos', idade: 18 }, { nome: 'Ana', idade: 25 }, { nome: 'Maria', idade: 30 }]
